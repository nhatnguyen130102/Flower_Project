@model IEnumerable<Flower_Models.MaterialType>

@{
    ViewData["Title"] = "Index";
    Layout ="~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}


<h1>Index</h1>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ID_MaterialType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name_MaterialType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ID_MaterialType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name_MaterialType)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ID_MaterialType }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ID_MaterialType }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ID_MaterialType })
                </td>
            </tr>
        }
    </tbody>
</table> 

