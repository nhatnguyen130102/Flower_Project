@model Flower_Models.StockReceivedDocket

@{
    ViewData["Title"] = "Create";
    Layout ="~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Create</h1>

<h4>StockReceivedDocket</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
          
            <div class="form-group">
                <label asp-for="ID_Shop" class="control-label"></label>
                <select asp-for="ID_Shop" class="form-control" asp-items="@ViewBag.Shop">
                    <option>Select Shop</option>
                </select>
                <span asp-validation-for="ID_Shop" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" asp-controller="StockReceivedDocket">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
